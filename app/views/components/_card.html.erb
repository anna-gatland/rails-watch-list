  <% if list.photo.attached? %>
    <div class="card col-4 m-2" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')">
      <%= list.name %><%= link_to "More", list_path(list) %>
    </div>
  <% else %>
    <div class="card col-4 m-2" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('')">
      <%= list.name %><%= link_to "More", list_path(list) %>
    </div>
  <% end %>
